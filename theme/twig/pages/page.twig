{% extends 'template/template.twig' %}

{% block content %}
	{% block page_image %}
		{{ page.image.tag('content-image', page.imageDescription)|raw }}
	{% endblock %}
	<div class="content-page">
		{% block content_title %}
		<h1>{{ page.getMainHeading() }}</h1>
		{% endblock %}
		{% block page_content %}
			{{ page.getPageContent()|expandHtml }}
		{% endblock %}
	</div>
{% endblock %}
