{% extends 'template/template.twig' %}

{% block content %}
	{% block page_image %}
		{{ page.image.tag('content-image', page.imageDescription)|raw }}
	{% endblock %}
	<div class="content__inner">
		<div class="container">
			<div class="main-content">
				{% block content_title %}
					<h1>{{ page.getMainHeading() }}</h1>
				{% endblock %}
				{% block page_content %}
					{{ page.getPageContent()|expandHtml }}
				{% endblock %}
			</div>
			<div class="main-link">
				{% for group in page.group_links %}
				<div class="col">
					<h3>{{group.label}}</h3>
					<ul>
						{% for link in group.links %}
						<li>
							{% include 'widgets/links/custom-link.twig' with {
								link: link,
							} %}
						</li>
						{% endfor %}
					</ul>
				</div>
				{% endfor %}
			</div>
		</div>
	</div>
{% endblock %}

{% block class_body %}health-professionals-page{% endblock %}
{% block class_content %}{% endblock %}
